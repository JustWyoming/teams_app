
<div class="col-md-4 col-md-offset-4">
<h1>Edit team</h1>

<%= form_for :team, url: team_path(@team), method: :patch do |f| %>

  "Title:" <%= f.text_field :title, :placeholder => "team Title", :class => "form-control" %>
   "Description:" <%= f.text_field :desc, :placeholder => "team Description", :class => "form-control" %>
   "location:" <%= f.text_field :loc, :placeholder => "team location", :class => "form-control" %>

   <%= f.submit :class=>'btn btn-primary'%>

<% end %>

<%= link_to 'Back', teams_path %>

</div>
</div>
</div>
